{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 6001",
    "dev:https": "HTTPS=true PORT=4300 node ./server.js",
    "build": "yarn build:css && next build",
    "build:css": "NODE_ENV=production tailwindcss -o out/tailwind.css -m",
    "start": "next start",
    "lint": "run-s lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "next lint",
    "lint:tsc": "tsc --noEmit",
    "format": "run-s format:prettier format:eslint",
    "format:prettier": "prettier --write --loglevel silent src",
    "format:eslint": "next lint --fix",
    "organize": "yarn lint && yarn format",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@next/font": "^13.0.4",
    "express": "^4.18.2",
    "next": "13.0.4",
    "next-auth": "^4.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cssnano": "^5.1.14",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-tailwindcss": "^3.7.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "spotify-types": "^1.0.7",
    "storybook": "^6.5.16",
    "typescript": "4.9.3",
    "vitest": "^0.28.4",
    "yarn-run-all": "^3.1.1"
  }
}
